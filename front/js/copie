let data  = [1,2,3,4,5,6,7,8,9];

console.log(data);

const index = data.findIndex((e) => e === 8);
console.log(index);
if (index > -1) {
    data.splice(index, 1);
}

console.log(data);


getLocalStorageCart;

const index = document.querySelector(".deleteItem");
getLocalStorageCart.forEach(element => {
    index.addEventListener("change", (e) => {
    e.preventDefault();//annulation de tout les comportements par defaut
    getLocalStorageCart.splice()
    })
  })
